<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane blendMode="MULTIPLY" prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: C5FFB5C5FFB5;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.varun.controller.HomeSceneController">
   <children>
      <VBox alignment="CENTER" prefHeight="900.0" prefWidth="600.0" spacing="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="courseDetailsButton" defaultButton="true" mnemonicParsing="false" onAction="#onCourseDetailsButtonClicked" prefHeight="69.0" prefWidth="200.0" text="Course Details" textAlignment="CENTER">
               <font>
                  <Font name="Adobe Clean Medium" size="20.0" />
               </font>
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor>
            </Button>
            <Button fx:id="studentDetailsButton" defaultButton="true" mnemonicParsing="false" onAction="#onStudentDetailsButtonClicked" prefHeight="69.0" prefWidth="200.0" text="Student Details" textAlignment="CENTER">
               <font>
                  <Font name="Adobe Clean Medium" size="20.0" />
               </font>
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor>
            </Button>
            <Button fx:id="notificationsButton" defaultButton="true" mnemonicParsing="false" onAction="#onNotificationButtonClicked" prefHeight="69.0" prefWidth="200.0" text="Notifications" textAlignment="CENTER">
               <font>
                  <Font name="Adobe Clean Medium" size="20.0" />
               </font>
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
